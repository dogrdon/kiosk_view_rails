<% provide(:title, "Log in | ") %>

<div class="container">
	<h2> Log in </h2>
	<div class="row">
	  <div class="col-lg-6 offset3">
	    <%= form_for(:session, url: sessions_path) do |f| %>

	      <%= f.label :email %><br/>
	      <%= f.text_field :email %><br/>

	      <%= f.label :password %><br/>
	      <%= f.password_field :password %><br/>
	      <br/>
	      <%= f.submit "Log in", class: "btn btn-large btn-primary" %>

	      
	    <% end %>
	  </div>
	</div>
	
</div>